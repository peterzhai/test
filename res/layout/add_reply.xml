<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<LinearLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:id="@+id/linearLayout1"
		android:orientation="vertical">
		<TextView
			android:layout_height="wrap_content"
			android:id="@+id/nums_tv"
			android:layout_width="wrap_content"
			android:text="回复号码"></TextView>
		<EditText
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:id="@+id/num_editText"></EditText>
	</LinearLayout>
	<LinearLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:id="@+id/linearLayout2"
		android:orientation="vertical">
		<TextView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:text="关键字"
			android:id="@+id/key_id_tv"></TextView>
		<EditText
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:id="@+id/key_editText">
		</EditText>
	</LinearLayout>
	<LinearLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:orientation="vertical"
		android:id="@+id/LinearLayout01">
		<TextView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:text="回复内容"
			android:id="@+id/回复内容"></TextView>
		<EditText
			android:layout_height="wrap_content"
			android:layout_width="match_parent"
			android:id="@+id/re_content_editText"></EditText>
	</LinearLayout>

</LinearLayout>
